<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="member">

	<insert id="memberEnroll">
	
		insert into
			member
		values(
			#{id}
			,#{name}
			,#{pwd}
			,#{birthDate}
			,#{nick}
			,default
			,null
		)
	</insert>
	
</mapper>