<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="post">

<!-- 게시물 추가  -->
<insert id="insertPost">

insert into
	post
values(
	seq_post_post_no.nextval
	,#{id}
	,#{boardCode}
	,default
	,default
	,#{title}
	,#{content}

)

</insert>
	
	
</mapper>

